{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["feature", "features"]
    },
    {
      "title": "## 🐛 Fixes",
      "labels": ["fix", "fixes"]
    },
    {
      "title": "## 🧪 Other",
      "labels": ["other"]
    }
  ],
  "sort": "ASC",
  "template": "${{CHANGELOG}}",
  "pr_template": "- ${{TITLE}}\n",
  "empty_template": "- no changes",
  "label_extractor": [
    {
      "pattern": "\\[(.*)\\].*",
      "target": "$1"
    }
  ],
  "transformers": [
    {
      "pattern": "\\[.*\\] (.*)",
      "target": "$1"
    }
  ],
  "max_tags_to_fetch": 200,
  "max_pull_requests": 200,
  "max_back_track_time_days": 365
}