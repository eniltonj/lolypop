{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["feature"]
    },
    {
      "title": "## 🐛 Fixes",
      "labels": ["fix"]
    },
    {
      "title": "## 🧪 Other",
      "labels": ["other"]
    }
  ],
  "template": "${{CHANGELOG}}",
  "pr_template": "- ${{TITLE}}\n",
  "empty_template": "${{OWNER}}\n${{REPO}}\n${{FROM_TAG}}\n${{TO_TAG}}",
  "label_extractor": [
    {
      "pattern": "([.]).",
      "target": "$1"
    }
  ]
}