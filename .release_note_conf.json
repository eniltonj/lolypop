{
  "categories": [
    {
      "title": "## ğŸš€ Features",
      "labels": ["feature"]
    },
    {
      "title": "## ğŸ› Fixes",
      "labels": ["fix"]
    },
    {
      "title": "## ğŸ§ª Tests",
      "labels": ["test"]
    }
  ],
  "ignore_labels": [
    "ignore"
  ],
  "sort": "ASC",
  "template": "${{CHANGELOG}}",
  "pr_template": " ",
  "empty_template": "- no changes",
  "label_extractor": [
    {
      "pattern": "(.) (.+)",
      "target": "$1\n $2\n"
    }
  ],
  "transformers": [
    {
      "pattern": "[\\-\\*] (\\[(...|feature|fix|other)\\])( )?(.+?)\n(.+?[\\-\\*] )(.+)",
      "target": "$1\n $2\n $3\n $4\n $5\n $6\n"
    }
  ],
  "max_tags_to_fetch": 200,
  "max_pull_requests": 0,
  "max_back_track_time_days": 365,
  "exclude_merge_branches": [
    "Owner/qa"
  ],
  "tag_resolver": {
    "method": "semver"
  }
}